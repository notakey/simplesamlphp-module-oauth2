{
    "name": "notakey/simplesamlphp-module-oauth2",
    "description": "A SimpleSAMLphp module adding support for the OAuth 2.0 protocol.",
    "type": "simplesamlphp-module",
    "keywords": ["oauth2"],
    "license": "MIT",
    "authors": [{
        "name": "Ingemars Asmanis",
        "email": "ingemars.asmanis@notakey.com"
    }],
    "autoload": {
        "psr-0": {
            "SimpleSAML\\Module\\oauth2\\": "lib"
        }
    },
    "require": {
        "php": ">=7.2.0",
        "simplesamlphp/composer-module-installer": "~1.0",
        "league/oauth2-server": "~8.1",
        "nette/forms": "~2.4",
        "laminas/laminas-diactoros": "~2.3",
        "laminas/laminas-httphandlerrunner": "~1.1"
    },
    "require-dev": {
        "simplesamlphp/simplesamlphp": "dev-master as 2.0",
        "simplesamlphp/saml2": "dev-master#00e38f85b417be1e10a2d738dd2f5ea82edb472c as 2.2"
    }
}